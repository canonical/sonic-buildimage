case $reason in
    BOUND|RENEW|REBIND|REBOOT)
        if [ -n "${new_minigraph_service_url}" ]; then
            echo $new_minigraph_service_url > /etc/sonic/graph_service_url
        else
            if [ !-f "/etc/sonic/graph_service_url"]; then
               echo "N/A" > /etc/sonic/graph_service_url
            fi
        fi
        ;;
esac
