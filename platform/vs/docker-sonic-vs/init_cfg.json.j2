{
    "DEVICE_METADATA": {
        "localhost": {
            "mac": "{{ system_mac }}",
            "buffer_model": "traditional",
            "yang_config_validation": {% if enable_yang_config_validation == "y" %}"enable"{% else %}"disable"{% endif %}
        }
    },
{% set features = ["swss", "bgp", "teamd", "nat", "database", "lldp", "dhcp_relay", "macsec"] %}
    "FEATURE": {
{% for feature in features %}
        "{{ feature }}": {
            "state": "enabled"
        }{% if not loop.last %},{% endif %}
{% endfor %}
    }
}

