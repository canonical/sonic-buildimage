
DPATH       := $($(DOCKER_PTF_PY3)_PATH)
DEP_FILES   := $(SONIC_COMMON_FILES_LIST) platform/vs/docker-ptf-py3.mk platform/vs/docker-ptf-py3.dep
DEP_FILES   += $(SONIC_COMMON_BASE_FILES_LIST)
