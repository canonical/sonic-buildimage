+-- Specification.
+
+
+IMPORTS
+    MODULE-IDENTITY             FROM SNMPv2-SMI
+    brcmSonicMibs               FROM BROADCOM-SONIC-PRODUCT-MIB;
+    
+    brcmSonicConfMgmt MODULE-IDENTITY
+        LAST-UPDATED "202004200000Z" -- 20 April 2020 12:00:00 GMT
+        ORGANIZATION "Broadcom"
+        CONTACT-INFO
+          "                      Customer Support
+          Postal:                Broadcom 
+                                 1030 Swabia Court
+                                 Suite 400
+                                 Durham, NC 27703
+          Tel:                   +1 919 865 2700"
+        DESCRIPTION
+          "This mib module defines objects used for managing the
+		   configuration of Broadcom Sonic devices."
+           
+        -- Revision history.
+        REVISION
+          "202004200000Z" -- 20 April 2020 12:00:00 GMT
+        DESCRIPTION
+          "Added configuration change notification"
+          
+    ::= {  brcmSonicMibs 1 }
+
+ 
+ --Subtree for defining mib objects
+ brcmSonicConfMgmtObjects                  OBJECT IDENTIFIER ::= { brcmSonicConfMgmt 1}
+
+ --Subtree for defining notifications
+ brcmSonicConfMgmtNotificationPrefix       OBJECT IDENTIFIER ::= { brcmSonicConfMgmt 2}
+ brcmSonicConfMgmtNotifications            OBJECT IDENTIFIER ::= { brcmSonicConfMgmtNotificationPrefix 0}
+
+ brcmSonicConfigChange NOTIFICATION-TYPE
+       STATUS  current
+       DESCRIPTION
+		"This notification is generated when running configuration was changed.
+		 Snmpd does not generate notification immediately. It waits for 30 secs
+		 for another config change to avoid multiple notifications in case config
+		 is changed within 30 sec. If there is no config change within 30 sec then 
+		 trap is generated.
+		 "
+	::= { brcmSonicConfMgmtNotifications 1 }
+
+
+END
