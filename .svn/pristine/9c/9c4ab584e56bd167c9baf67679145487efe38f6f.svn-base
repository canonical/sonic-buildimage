[Unit]
Description=Alphanetworks SNJ61D0-320F Platform initialization service
Before=pmon.service
DefaultDependencies=no

[Service]
Type=oneshot
ExecStart=/usr/local/bin/alphanetworks_snj61d0_util.py -f install
ExecStop=/usr/local/bin/alphanetworks_snj61d0_util.py clean
RemainAfterExit=yes

[Install]
WantedBy=multi-user.target
