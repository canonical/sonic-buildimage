bcm_device:
    0:
        global:
            uft_mode: 1
            l3_aplm_template: 1
            l2_learn_enable: 1
#            vlan_flooding_l2mc_num_reserved: 0
            pktio_mode: 0
            bcm_tunnel_term_compatible_mode: 1
            l3_alpm_template: 2
            l3_alpm2_bnk_threshold: 100
            l3_enable: 1
            l2_hitbit_enable: 0
...
